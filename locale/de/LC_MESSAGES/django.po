#, python-format
msgid ""
"\n"
"<p>Dear %(user_name)s,</p>\n"
"\n"
"<p>The import of users failed. Here is the error message of the server:</p>\n"
"\n"
"<p>%(error_message)s</p>\n"
msgstr ""
"\n"
"<p>Sehr geehrter %(user_name)s,</p>\n"
"\n"
"<p>Der Import von Benutzern ist fehlgeschlagen. Hier ist die Fehlermeldung des Servers:</p>\n"
"\n"
"<p>%(error_message)s</p>\n"

